{
  "name": "steedos-project-template",
  "version": "0.0.1",
  "private": true,
  "workspaces.disabled": [
    "steedos-app",
    "node-red-app"
  ],
  "description": "",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "yarn start:steedos && yarn start:app",
    "start:steedos": "cross-env NODEID=STEEDOS-SERVER moleculer-runner services/steedos-server",
    "start:app": "cross-env NODEID=STEEDOS-APP moleculer-runner --repl services/steedos-app",
    "console": "cross-env NODEID=STEEDOS-CONSOLE moleculer connect --config ./moleculer.config.js",
    "docker": "docker-compose up --build -d"
  },
  "keywords": [
    "steedos",
    "low-code",
    "microservices",
    "moleculer"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@steedos/accounts": "1.23.20",
    "@steedos/plugin-enterprise": "1.23.20",
    "@steedos/plugin-qywx": "^0.0.17",
    "@steedos/service-metadata": "^1.0.0",
    "@steedos/service-package-loader": "0.0.1",
    "@steedos/service-steedos-server": "0.0.1",
    "@steedos/steedos-plugin-workflow": "2.1.1",
    "@steedos/word-template": "1.23.0-alpha.17",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^3.1.0",
    "ioredis": "^4.22.0",
    "moleculer": "^0.14.12",
    "moleculer-cli": "^0.7.1",
    "mssql": "5.1.2",
    "mysql": "2.18.1",
    "oracledb": "4.0.1",
    "pg": "7.18.2",
    "redlock": "^4.2.0",
    "sqlite3": "4.2.0",
    "steedos-server": "1.23.20"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "moleculer-repl": "^0.6.4"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  },
  "homepage": "https://www.steedos.org",
  "resolutions": {
    "**/lodash": "^4.17.15",
    "**/handlebars": "^4.3.0",
    "**/debug": "^3.1.0",
    "**/hoek": "^5.0.3",
    "**/cryptiles": "^4.1.2",
    "**/bcrypt": "3.0.6",
    "**/samlp": "3.4.1",
    "**/xmldom": "0.2.1",
    "**/tslib": "1.11.2",
    "**/fibers": "^4.0.3",
    "**/cookies": "0.6.2"
  }
}
